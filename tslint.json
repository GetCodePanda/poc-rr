{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules":{
    "no-console": false,
    "no-shadowed-variable": false,
    "ordered-imports":false,
    "jsx-wrap-multiline": false,
    "jsx-boolean-value":false,
    "no-string-literal":false
  }
}
